def is_digit(c char) bool
{
	difference int32 = int32('9') - int32(c);
	return difference >= 0 && difference <= 9;
}

def char_to_int(c char) int32
{
	return int32(c - '0');
}

n int32 = int32(input());
s char[] = input();

total_sum int32 = 0;
local_sum int32 = 0;

for (i int32 = 0; i < n; i += 1)
{
	if (is_digit(s[i]))
	{
		local_sum = (local_sum * 10) + char_to_int(s[i]);
	}
	else
	{
		total_sum += local_sum;
		local_sum = 0;
	}
}

print(total_sum);
