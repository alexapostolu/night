print("Hello, Night!\n");

#
# Expressions, Variables and Arrays
#

print(str(2.0 + 3.4) + "\n");

my_var0 int32;
print(my_var0);

my_var1 int32 = 1;
print(my_var1);

my_var1 = 2 + 3;
print(my_var1);

my_arr float[2] = [ 0.0, 1.0 ];
print(my_arr[0]);
print(my_arr[1]);

my_arr[0] = -5.0;
print(str(my_arr[0] + my_arr[1] + 2.4) + "\n");

#
# Conditionals
#

my_cond bool = false;
if (!my_cond)
{
	print("false");
	my_cond = true;
}
else if (my_cond)
{
	print("true");
}

if (my_cond)
	print("true\n");
else
	print("false");

#
# Loops
#

for (i int32 = 0; i < 5; i += 2)
	print(str(i) + " ");
print("\n");

i int32 = 0;
while (i != 4)
{
	print(str(i) + " ");
	i += 1;
}
print('\n');

#
# Functions
#

def my_func(my_param1 int32, my_param2 char[]) char[]
{
	return str(my_param1) + my_param2;
}

print(my_func(5, "0") + "\n");
